<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.proyectofinal">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.hardware.sensor.accelerometer" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrarActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".ContabilizadorActivity" />
        <activity
            android:name=".listaGymActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".DetalleGymActivity"
            android:label="@string/title_activity_detalle_gym"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".SentadillasActivity" />
        <activity android:name=".InstrctorActivity"></activity>
    </application>

</manifest>